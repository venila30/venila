#include<stdio.h>   
int main()
{
    int i,j,a[3][3],b[3][3],c[3][3];
    printf("\n enter the a values");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%",&b[i][j]);
        }
    }
    printf("\n enter the b values");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            c[i][j]=a[i][j]+b[i][j];
        }
    }
    for(i=0;i<3;i++)
    {
        printf("\n");
        for(j=0;j<3;j++)
        {
            printf("%d",b[i][j]);
        }
    }
    return 0;
}
